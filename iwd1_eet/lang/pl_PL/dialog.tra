@18792 = ~Pierścień siły~
@18793 = ~Przeklęta machina! Nigdy nie uda mi się jej naprawić!~
@18794 = ~Mogę spytać co z *nią* jest nie tak?~
@18795 = ~~
@18796 = ~Kaylessa dała mi tę część maszyny. Może właśnie tego szukasz?~
@18797 = ~Tę część odnaleźliśmy w Wieży Solonora. Czy mógłbyś naprawić machinę przy jej użyciu?~
@18798 = ~Znalazłem ten fragment w jednej z sali wojennych. Może będzie użyteczny.~ ~Znalazłam ten fragment w jednej z sali wojennych. Może będzie użyteczny.~
@18799 = ~Zabrałem ten kawałek nieumarłemu cieniowi w Dłoni. Wygląda na część tej machiny.~ ~Zabrałam ten kawałek nieumarłemu cieniowi w Dłoni. Wygląda na część tej machiny.~
@18800 = ~Przeszkodziłem ci, wybacz. Wrócę później.~ ~Przeszkodziłam ci, wybacz. Wrócę później.~
@18801 = ~Nie widzisz, że jest zepsuta? Nie mogę jej uruchomić, dopóki nie odnajdę brakujących części.~
@18802 = ~Jakich brakujących części?~
@18803 = ~Możesz mi o sobie opowiedzieć?~
@18804 = ~Może jak wrócę później, poczynisz większe postępy. Żegnaj.~
@18805 = ~Nie mam czasu na rozmowy z technicznymi ignorantami! Znajdź część, która pasuje albo zejdź mi z drogi!~
@18806 = ~AAACH! Marnujesz mój czas bezcelową gadaniną. Mam na imię Geralith. W czasach gdy jeszcze żyłem, zbudowałem tę kupę złomu, która kiedyś była astrolabium.~
@18807 = ~Jak to się stało, że tutaj skończyłeś, gdy upadła Dłoń?~
@18808 = ~Ty zbudowałeś astrolabium?~
@18809 = ~Przepraszam, że zmarnowałem twój czas. Żegnaj.~ ~Przepraszam, że zmarnowałam twój czas. Żegnaj.~
@18810 = ~Owszem. Larrel obarczył mnie odpowiedzialnością za zaprojektowanie i zbudowanie tej machiny. Potrzebna była czarodziejom do pomocy w badaniach nad gwiazdami i księżycami. Teraz nadaje się tylko do magazynowania kurzu.~
@18811 = ~Jak to się stało, że tutaj skończyłeś, gdy upadła Dłoń?~
@18812 = ~Może Larrel będzie coś wiedział?~
@18813 = ~~
@18814 = ~Ostatnią rzeczą, jaką pamiętam z czasów obrony Dłoni, jest strzała przebijająca moje plecy. Kiedy się ocknąłem, zrozumiałem, że stałem się duchem. Teraz mój czas upływa na próbach znalezienia sposobu na naprawienie tej przeklętej machiny.~
@18815 = ~Wspomniałeś wcześniej, że zbudowałeś astrolabium?~
@18816 = ~modyfikuj postacie~
@18817 = ~Może później będę mógł ci pomóc. Żegnaj.~ ~Może później będę mogła ci pomóc. Żegnaj.~
@18818 = ~Rozmawialiście z nim? Wygłasza tylko bezsensowne przemowy. Jego działania nie mają żadnego sensu. Nie ma wiele wspólnego ze szlachetnym czarodziejem, jakim był kiedyś.~
@18819 = ~Jak to się stało, że tutaj skończyłeś, gdy upadła Dłoń?~
@18820 = ~Mówiłeś, że zbudowałeś astrolabium?~
@18821 = ~Może uda mi się wydobyć z niego coś sensownego. Żegnaj.~
@18822 = ~A niech mnie Otchłań pochłonie. Możesz mieć rację. Jeśli znajdziesz jeszcze jakieś części, daj mi znać. Poczekaj chwilę, zaraz to zainstaluję.~
